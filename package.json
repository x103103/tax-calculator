{
  "name": "tax-calculator",
  "version": "1.0.0",
  "description": "A tax calculator for various countries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node app/test-poland-tax.ts",
    "start": "node dist/test-poland-tax.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "pnpm run type-check && husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/x103103/tax-calculator"
  },
  "engines": {
    "node": ">=24.13.0",
    "pnpm": ">=10.28.0"
  },
  "keywords": [
    "tax",
    "calculator",
    "poland"
  ],
  "author": "Oleksandr Oliinyk <x103103@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.9",
    "eslint": "^9.39.2",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "^29.12.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
